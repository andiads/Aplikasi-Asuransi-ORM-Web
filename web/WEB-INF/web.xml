<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DataAsuransiServlet</servlet-name>
        <servlet-class>controller.DataAsuransiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataPembayaranServlet</servlet-name>
        <servlet-class>controller.DataPembayaranServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportNasabahServlet</servlet-name>
        <servlet-class>controller.ReportNasabahServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportKlaimServlet</servlet-name>
        <servlet-class>controller.ReportKlaimServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportPembayaranServlet</servlet-name>
        <servlet-class>controller.ReportPembayaranServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataAsuransiServlet</servlet-name>
        <url-pattern>/dataasuransiservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataPembayaranServlet</servlet-name>
        <url-pattern>/datapembayaranservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportNasabahServlet</servlet-name>
        <url-pattern>/reportNasabahServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportKlaimServlet</servlet-name>
        <url-pattern>/reportKlaimServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportPembayaranServlet</servlet-name>
        <url-pattern>/reportPembayaranServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
